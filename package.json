{
  "private": true,
  "name": "blob",
  "version": "1.0.0",
  "description": "Create via a POST to /blob - Anything you send will create a new entry",
  "type": "module",
  "main": "src/server.ts",
  "scripts": {
    "start": "node --experimental-vm-modules --experimental-top-level-await build/server.js",
    "build": "tsc --outDir ./build/",
    "dev": "nodemon -w 'reference/**/*.yaml' -w 'src/**/*.ts' --exec node -- -r 'dotenv/config' --experimental-vm-modules --experimental-top-level-await --loader ts-node/esm.mjs src/server.ts",
    "lint": "eslint -c .eslintrc.json --ext .ts src/**/*.ts"
  },
  "author": "James Ward <james@notjam.es>",
  "license": "MIT",
  "engines": {
    "node": ">=13"
  },
  "engineStrict": true,
