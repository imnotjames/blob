    "@types/http-errors": "^1.8.0",
    "@types/ioredis": "^4.17.2",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-static": "^4.0.1",
    "@types/koa__router": "^8.0.2",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^14.0.26",
    "@types/raw-body": "^2.3.0",
    "@types/redlock": "^4.0.1",
    "@types/uuid": "^8.0.0",
    "@types/yamljs": "^0.2.31",
