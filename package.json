    "@koa/cors": "^3.1.0",
    "@types/koa__cors": "^3.0.1",
    "dotenv": "^8.2.0",
    "env-var": "^6.3.0",
    "http-errors": "^1.8.0",
    "http-link-header": "^1.0.2",
    "ioredis": "^4.17.3",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-static": "^5.0.0",
    "lru-cache": "^6.0.0",
    "pkg-up": "^3.1.0",
    "raw-body": "^2.4.1",
    "redlock": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.3.0",
    "uuid": "^8.3.0",
    "yamljs": "^0.3.0"
